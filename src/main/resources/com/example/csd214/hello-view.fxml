<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="378.0" prefWidth="528.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd214.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <TableView fx:id="classlist" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="Studentid" prefWidth="75.0" text="Studentid" />
            <TableColumn fx:id="Studentname" prefWidth="75.0" text="Studentname" />
            <TableColumn fx:id="Studentphonenumber" prefWidth="75.0" text="Studentphonenumber" />
            <TableColumn fx:id="Teachername" prefWidth="75.0" text="Teachername" />
        </columns>
    </TableView>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
